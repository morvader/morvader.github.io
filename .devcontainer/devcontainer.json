{
    "name": "Hugo Blog",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "forwardPorts": [1313],
    "settings": {
        "terminal.integrated.defaultProfile.linux": "ash"
    },
    "extensions": [
        "yzhang.markdown-all-in-one",
        "budparr.language-hugo-vscode",
        "eamodio.gitlens",
        "mhutchie.git-graph"
    ],
    "remoteUser": "root",
    "postCreateCommand": "hugo version && git --version"
}